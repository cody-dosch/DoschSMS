
@{
    ViewBag.Title = "Drop Course";
}

@model StudentManagementSystem.Models.CourseModel

<div class="form-header">
    <h2>Drop Complete</h2>
    <br />
    <p>Successfully dropped <b>@Model.Department@Model.CourseNumber</b>!</p>
    <button type="button" class="btn btn-info" name="backBtn" id="backBtn" onclick="window.location.href='@Url.Action("MyCourses", "Courses")'">Back</button>
</div>
